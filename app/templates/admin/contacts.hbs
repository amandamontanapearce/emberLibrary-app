<h1>Contact Messages</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Email</th>
      <th>Message</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |contact|}}
    <tr>
      <th>
        {{contact.email}}
        <button class="btn btn-danger btn-xs" {{action 'deleteContact' contact}}>Delete</button>
      </th>
      <td>{{contact.message}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>
